{
  "name": "2long url shortener",
  "version": "0.1",
  "description": "Extension that makes your current url shorter.",
  "icons": {"128": "icon_128.png", "64": "icon_64.png", "48": "icon_48.png"},
  "browser_action": {
      "default_icon": "icon_128.png",
      "popup": "popup.html"
  },
  "permissions": [
      "http://2long.ru/*",
      "tabs"
  ],
  "background_page": "background.html",
  "homepage_url": "http://2long.ru/"

}
